<div class="<%= contentclass %>">
  <div class="<%= contentinnerclass %>">
    <h1><%= title %></h1>
    <% if ((typeof(login) !== 'undefined') && (typeof(email) !== 'undefined')) {%>
      <input type="text" placeholder="<%= login %>" readonly style="width: 15rem; height: 1.5rem" name="login"/><br><br>
      <input type="text" placeholder="<%= email %>" readonly style="width: 15rem; height: 1.5rem" name="email"/><br><br>
      <input type="submit" value="Edit" style="width: 15rem; height: 1.5rem"/><br><br>
    <%}%>
  </div>
</div>
